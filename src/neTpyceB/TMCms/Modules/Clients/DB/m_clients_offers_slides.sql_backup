CREATE TABLE `m_clients_offers_slides` (  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,  `offer_id` int(10) unsigned NOT NULL,  `template` varchar(255) NOT NULL,  `order` int(10) unsigned NOT NULL,  PRIMARY KEY (`id`),  KEY `offer_id` (`offer_id`),  CONSTRAINT `m_clients_offers_id_slides` FOREIGN KEY (`offer_id`) REFERENCES `m_clients_offers` (`id`) ON DELETE CASCADE ON UPDATE CASCADE) ENGINE=InnoDB DEFAULT CHARSET=utf8