CREATE TABLE `m_clients_collections` (  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,  `group_id` int(10) unsigned NOT NULL,  `active` tinyint(3) unsigned NOT NULL DEFAULT '0',  `title` int(10) unsigned NOT NULL COMMENT 'dstring',  `templates_count` tinyint(3) unsigned NOT NULL,  PRIMARY KEY (`id`),  KEY `group_id` (`group_id`)) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8