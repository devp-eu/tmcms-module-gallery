CREATE TABLE `m_clients_offers` (  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,  `client_id` int(10) unsigned NOT NULL,  `collection_id` int(10) unsigned NOT NULL,  `date_added` int(10) unsigned NOT NULL,  `date_updated` int(10) unsigned NOT NULL,  `title` varchar(255) NOT NULL,  PRIMARY KEY (`id`),  KEY `client_id` (`client_id`),  KEY `collection_id` (`collection_id`),  CONSTRAINT `m_clients_offers_collection_id` FOREIGN KEY (`collection_id`) REFERENCES `m_clients_collections` (`id`) ON DELETE CASCADE ON UPDATE CASCADE) ENGINE=InnoDB AUTO_INCREMENT=132 DEFAULT CHARSET=utf8